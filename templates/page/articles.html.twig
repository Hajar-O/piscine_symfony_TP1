{% extends 'base.html.twig' %}
{% block content %}
<h1>Articles list</h1>
<section class="allArticle">

    {% for pokemon in pokemons %}

        {% if pokemon.isPublished == true %}

        <article class="myArticle">
            <h2>{{ pokemon.title }}</h2>
            <p>{{ pokemon.content }}</p>
        </article>
    {% endif %}
    {% endfor %}
</section>

{% endblock %}
