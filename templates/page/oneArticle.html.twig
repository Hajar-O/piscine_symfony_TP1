{% extends 'base.html.twig' %}
{% block meta_title %}
    Pokemon : {{ pokemon.title }}
{% endblock meta_title %}
{% block content %}

    <article class="show_art">
        <h1>{{ pokemon.title }}</h1>
        <p>{{ pokemon.content }}</p>
        <img src="{{ asset(pokemon.img) }}" alt="">
    </article>


{% endblock %}